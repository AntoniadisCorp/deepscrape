{
  "/api/anthropic": {
    "target": "https://api.anthropic.com",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/anthropic": "/v1"
    }
  },
  "/geo": {
    "target": "https://nominatim.openstreetmap.org",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/geo": ""
    }
  },
  "/api/openai": {
    "target": "https://api.openai.com",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/openai": "/v1"
    }
  },
  "/api/groq": {
    "target": "https://api.groq.com",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/groq": "/openai/v1"
    }
  },
  "/api/jina": {
    "target": "https://r.jina.ai",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/jina": ""
    }
  },
  "/recaptcha": {
    "target": "https://google.com",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/recaptcha": "/api"
    }
  },
  "/api/deploy": {
    "target": "http://127.0.0.1:8080",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/deploy": "/api/deploy"
    }
  },
  "/api": {
    "target": "http://localhost:8000",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": ""
    }
  },
  "/apa/*": {
    "target": "http://localhost:3400",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/apa": ""
    }
  }
}
